{
  "name": "compatto-furnishing",
  "version": "1.0.0",
  "description": "Compatto Furnishing - Simplifying the furnishing market",
  "main": "index.js",
  "scripts": {
    "dev": "tailwindcss -i ./assets/css/input.css -o ./docs/tailwind-output.css --watch",
    "build": "tailwindcss -i ./assets/css/input.css -o ./docs/tailwind-output.css --minify",
    "start": "npx http-server docs -p 3000 -o",
    "serve": "npm run start",
    "lint": "echo 'No linting configured yet'",
    "format": "echo 'No formatting configured yet'",
    "clean": "rm -rf docs/tailwind-output.css",
    "prebuild": "npm run clean"
  },
  "keywords": [
    "furnishing",
    "tailwindcss",
    "responsive",
    "design"
  ],
  "author": "Compatto Furnishing",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^10.4.20",
    "postcss": "^8.5.1",
    "tailwindcss": "^3.4.17"
  },
  "dependencies": {
    "http-server": "^14.1.1",
    "@stripe/stripe-js": "^5.6.0"
  }
}
